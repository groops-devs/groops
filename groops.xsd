						<xs:element name="outColumn" type="expression" minOccurs="0" default="[&quot;data0&quot;]" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>expression of output columns, extra &apos;epoch&apos; variable</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="statistics" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="outputfileInstrument" type="filename">
