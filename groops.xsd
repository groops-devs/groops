						<xs:element name="condition" type="conditionType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>check before each loop step</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="fileGnssStationInfo">
				<xs:annotation>
					<xs:documentation>DEPRECATED. Use platformEquipment instead</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileGnssStationInfo" type="filename">
							<xs:annotation>
								<xs:documentation>station/transmitter info file</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="infoType">
							<xs:annotation>
								<xs:documentation>info to loop over</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element name="antenna">
										<xs:annotation>
											<xs:documentation>loop over antennas</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="receiver">
										<xs:annotation>
											<xs:documentation>loop over receivers</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="variableLoopName" type="string" minOccurs="0" default="loopName">
							<xs:annotation>
								<xs:documentation>variable with antenna/receiver name</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopSerial" type="string" minOccurs="0" default="loopSerial">
							<xs:annotation>
								<xs:documentation>variable with antenna/receiver serial</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopInfo" type="string" minOccurs="0" default="loopInfo">
							<xs:annotation>
								<xs:documentation>variable with radome (antenna) or version (receiver)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopTimeStart" type="string" minOccurs="0" default="loopTimeStart">
							<xs:annotation>
								<xs:documentation>variable with antenna/receiver start time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopTimeEnd" type="string" minOccurs="0" default="loopTimeEnd">
							<xs:annotation>
								<xs:documentation>variable with antenna/receiver end time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopIndex" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>variable with index of current iteration (starts with zero)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopCount" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>variable with total number of iterations</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="condition" type="conditionType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>check before each loop step</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="magnetosphereType">
		<xs:annotation>
			<xs:documentation>magentic field of the Earth</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="igrf">
				<xs:annotation>
					<xs:documentation>International Geomagnetic Reference Field</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileMagneticNorthPole" type="filename" minOccurs="0" default="{groopsDataDir}/magnetosphere/magneticNorthPole.txt">
							<xs:annotation>
								<xs:documentation>time series of north pole</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="matrixGeneratorType">
		<xs:annotation>
			<xs:documentation>matrix calculation</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="file">
				<xs:annotation>
					<xs:documentation>from file</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileMatrix" type="filename"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="normalsFile">
				<xs:annotation>
					<xs:documentation>from normal equation file</xs:documentation>
					<xs:appinfo>rename: inputfileNormalequation = inputfileNormalEquation</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileNormalEquation" type="filename"/>
						<xs:element name="type">
							<xs:complexType>
								<xs:choice>
									<xs:element name="normalMatrix"/>
									<xs:element name="rightHandSide"/>
									<xs:element name="lPl"/>
									<xs:element name="observationCount"/>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="expression">
				<xs:annotation>
					<xs:documentation>matrix filled by an expression</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rows" type="expression">
							<xs:annotation>
								<xs:documentation>(variables: rowsBefore, columnsBefore)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="columns" type="expression">
							<xs:annotation>
								<xs:documentation>(variables: rowsBefore, columnsBefore)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="element" type="expression" default="if(row==column,1,0)">
							<xs:annotation>
								<xs:documentation>for each element of matrix (variables: row, column, rows, columns, rowsBefore, columnsBefore)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="elementManipulation">
				<xs:annotation>
					<xs:documentation>elements of a matrix are manipulated by an expression</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="element" type="expression" default="data">
							<xs:annotation>
								<xs:documentation>for each element of matrix (variables: data, row, column, rows, columns, rowsBefore, columnsBefore)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="elementWiseOperation">
				<xs:annotation>
					<xs:documentation>element wise operation of two matrices</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix1" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="matrix2" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="expression" type="expression" minOccurs="0" default="data0*data1">
							<xs:annotation>
								<xs:documentation>for each element of matrix (variables: data0, data1, row, column, rows, columns, rowsBefore, columnsBefore)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="append">
				<xs:annotation>
					<xs:documentation>append matrix to right or bottom</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="side">
							<xs:complexType>
								<xs:choice>
									<xs:element name="right"/>
									<xs:element name="bottom"/>
									<xs:element name="diagonal"/>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="shift">
				<xs:annotation>
					<xs:documentation>shift start row and/or start column</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="startRow" type="expression" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>start row (variables: rowsBefore, columnsBefore, rows, columns)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="startColumn" type="expression" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>start column (variables: rowsBefore, columnsBefore, rows, columns)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="slice">
				<xs:annotation>
					<xs:documentation>slice of a matrix</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="startRow" type="expression" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>start row of matrix (variables: rowsBefore, columnsBefore, rows, columns)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="startColumn" type="expression" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>start column of matrix (variables: rowsBefore, columnsBefore, rows, columns)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="rows" type="expression" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>0: until end (variables: rowsBefore, columnsBefore, rows, columns)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="columns" type="expression" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>0: until end (variables: rowsBefore, columnsBefore, rows, columns)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="reshape">
				<xs:annotation>
					<xs:documentation>matrix reshaped columnwise to new row and columns</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="rows" type="expression" default="0">
							<xs:annotation>
								<xs:documentation>0: auto-determine rows, (variables: rowsBefore, columnsBefore)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="columns" type="expression" default="0">
							<xs:annotation>
								<xs:documentation>0: auto-determine columns (variables: rowsBefore, columnsBefore)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="reorder">
				<xs:annotation>
					<xs:documentation>reorder matrix with index vectors</xs:documentation>
					<xs:appinfo>rename: rowIndex = inputfileIndexVectorRow</xs:appinfo>
					<xs:appinfo>rename: columnIndex = inputfileIndexVectorColumn</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="inputfileIndexVectorRow" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>index in input matrix or -1 for new parameter.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileIndexVectorColumn" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>index in input matrix or -1 for new parameter.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="sort">
				<xs:annotation>
					<xs:documentation>sort matrix by column</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="column" type="uint" default="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>sort by column, top = highest priority</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="descending" type="boolean" minOccurs="0" default="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="transpose">
				<xs:annotation>
					<xs:documentation>transposed of a matrix</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="multiplication">
				<xs:annotation>
					<xs:documentation>multiplication of matrices</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix1" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="matrix2" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="inverse">
				<xs:annotation>
					<xs:documentation>inverse/pseudoinverse</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="pseudoInverse" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>compute pseudo inverse instead of regular one</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="cholesky">
				<xs:annotation>
					<xs:documentation>cholesky decomposition</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="rankKUpdate">
				<xs:annotation>
					<xs:documentation>rank k update (A^T*A)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="eigenValues">
				<xs:annotation>
					<xs:documentation>eigen values</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="eigenVectors" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>return eigen vectors instead of eigen values</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="diagonal">
				<xs:annotation>
					<xs:documentation>diagonal of matrix</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="diagonal" type="int" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>zero: main diagonal, positive: superdiagonal, negative: subdiagonal</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="fromDiagonal">
				<xs:annotation>
					<xs:documentation>create matrix from diagonal vector</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>(nx1) or (1xn) diagonal vector</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="diagonal" type="int" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>zero: main diagonal, positive: superdiagonal, negative: subdiagonal</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="setType">
				<xs:annotation>
					<xs:documentation>set type of a matrix</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded"/>
						<xs:element name="type">
							<xs:complexType>
								<xs:choice>
									<xs:element name="matrix"/>
									<xs:element name="matrixSymmetricUpper"/>
									<xs:element name="matrixSymmetricLower"/>
									<xs:element name="matrixTriangularUpper"/>
									<xs:element name="matrixTriangularLower"/>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="miscAccelerationsType">
		<xs:annotation>
			<xs:documentation>non conservative forces acting on satellites</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="relativisticEffect">
				<xs:annotation>
					<xs:documentation>Relativistic effect (IERS2010)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="beta" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>PPN (parameterized post-Newtonian) parameter</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gamma" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>PPN (parameterized post-Newtonian) parameter</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="J" type="double" minOccurs="0" default="9.8e8">
							<xs:annotation>
								<xs:documentation>Earths angular momentum per unit mass [m**2/s]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="GM" type="double" minOccurs="0" default="3.986004415e+14">
							<xs:annotation>
								<xs:documentation>Geocentric gravitational constant</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="radiationPressure">
				<xs:annotation>
					<xs:documentation>Solar and Earh radiation pressure model</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="solarflux" type="double" minOccurs="0" default="1367">
							<xs:annotation>
								<xs:documentation>solar flux constant in 1 AU [W/m^2]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="eclipse" type="eclipseType"/>
						<xs:element name="inputfileAlbedoTimeSeries" type="filename" minOccurs="0" default="{groopsDataDir}/earthRadiationPressure/CERES_SYN1deg_albedo_climatology.dat">
							<xs:annotation>
								<xs:documentation>GriddedDataTimeSeries of albedo values (unitless)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileLongwaveFluxTimeSeries" type="filename" minOccurs="0" default="{groopsDataDir}/earthRadiationPressure/CERES_SYN1deg_longwaveFlux_climatology.dat">
							<xs:annotation>
								<xs:documentation>GriddedDataTimeSeries of longwave flux values [W/m^2]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factorSolarRadation" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>Solar radiation pressure is multiplied by this factor</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factorEarthRadation" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>Earth radiation preussure is multiplied by this factor</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factorThermalRadiation" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>Thermal (re-)radiation is multiplied by this factor</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="atmosphericDrag">
				<xs:annotation>
					<xs:documentation>atmospheric drag</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="thermosphere" type="thermosphereType"/>
						<xs:element name="earthRotation" type="double" minOccurs="0" default="7.29211585531e-5">
							<xs:annotation>
								<xs:documentation>[rad/s]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="considerTemperature" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>compute drag and lift, otherwise simple drag coefficient is used</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="considerWind" type="boolean" minOccurs="0" default="1"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="antennaThrust">
				<xs:annotation>
					<xs:documentation>antenna thrust</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="atmosphericDragFromDensityFile">
				<xs:annotation>
					<xs:documentation>atmospheric drag (from density along orbit)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileDensity" type="filename">
							<xs:annotation>
								<xs:documentation>density along orbit, MISCVALUE (kg/m^3)</xs:documentation>
							</xs:annotation>
						</xs:element>
