								<xs:documentation>*: left this part untouched, object</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="type" type="string" minOccurs="0" default="*">
							<xs:annotation>
								<xs:documentation>*: left this part untouched, type</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="temporal" type="string" minOccurs="0" default="*">
							<xs:annotation>
								<xs:documentation>*: left this part untouched, temporal representation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="interval" type="string" minOccurs="0" default="*">
