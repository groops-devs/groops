			<xs:element name="Grs2PotentialCoefficients">
				<xs:annotation>
					<xs:documentation>spherical harmonics from Geodetic Reference System (GRS)</xs:documentation>
					<xs:appinfo>tag: Misc</xs:appinfo>
					<xs:appinfo>tag: PotentialCoefficients</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfilePotentialCoefficients" type="filename"/>
						<xs:element name="maxDegree" type="uint" default="10"/>
						<xs:element name="GM" type="double" minOccurs="0" default="3.986005e+14">
							<xs:annotation>
								<xs:documentation>Geocentric gravitational constant</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378137.0">
							<xs:annotation>
								<xs:documentation>reference radius</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="omega" type="double" minOccurs="0" default="7292115e-11">
							<xs:annotation>
								<xs:documentation>Angular velocity of rotation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="J2" type="double" minOccurs="0" default="108263e-8">
							<xs:annotation>
								<xs:documentation>Dynamical form factor</xs:documentation>
