						<xs:element name="inputfileNormalEquation" type="filename"/>
						<xs:element name="inputfileParameter" type="filename">
							<xs:annotation>
								<xs:documentation>x</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>alpha</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="writeNormalMatrix" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>write full coefficient matrix, right hand sides and info files</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="NormalsRegularizationBorders">
				<xs:annotation>
					<xs:documentation>Two regularization matrices for inside und outside of Border</xs:documentation>
					<xs:appinfo>tag: NormalEquation</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileInside" type="filename"/>
						<xs:element name="outputfileOutside" type="filename"/>
						<xs:element name="grid" type="gridType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>nodal point distribution of parameters, e.g harmonics splines</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="border" type="borderType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>regularization areas, e.g land and ocean</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="NormalsRegularizationSphericalHarmonics">
				<xs:annotation>
					<xs:documentation>diagonal regularization matrix from gravity field accuracies, if not given from signal (cnm,snm), if not given from kaulas rule</xs:documentation>
					<xs:appinfo>tag: NormalEquation</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileDiagonalmatrix" type="filename"/>
						<xs:element name="gravityfield" type="gravityfieldType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>use sigmas, if not given use signal (cnm,snm), if not given use kaulas rule</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="minRegularizationDegree" type="uint" minOccurs="0"/>
						<xs:element name="maxRegularizationDegree" type="uint" minOccurs="0"/>
						<xs:element name="minDegree" type="uint" default="2"/>
						<xs:element name="maxDegree" type="uint"/>
						<xs:element name="numbering" type="sphericalHarmonicsNumberingType">
							<xs:annotation>
								<xs:documentation>numbering scheme for regul matrix</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="GM" type="double" minOccurs="0" default="3.986004415e+14">
							<xs:annotation>
								<xs:documentation>Geocentric gravitational constant</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378136.3">
							<xs:annotation>
								<xs:documentation>reference radius</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="makeIsotropic" type="boolean" minOccurs="0" default="0"/>
						<xs:element name="kaulaPower" type="double" minOccurs="0" default="2">
							<xs:annotation>
								<xs:documentation>sigma = kaulaFactor*degree**kaulaPower</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="kaulaFactor" type="double" minOccurs="0" default="1e-5">
							<xs:annotation>
								<xs:documentation>sigma = kaulaFactor*degree**kaulaPower</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="NormalsReorder">
				<xs:annotation>
					<xs:documentation>Reorder normal equations</xs:documentation>
					<xs:appinfo>rename: outputfileNormalequation = outputfileNormalEquation</xs:appinfo>
					<xs:appinfo>rename: inputfileNormalequation = inputfileNormalEquation</xs:appinfo>
