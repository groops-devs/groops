					<xs:documentation>matrix containing parameter indexes</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileMatrix" type="filename">
							<xs:annotation>
								<xs:documentation>index in old parameter list or -1 for new parameter</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="column" type="expression" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>use this column (counting from 0, variables: columns)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="startRow" type="expression" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>start at this row (counting from 0, variables: rows)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="countRows" type="expression" minOccurs="0">
							<xs:annotation>
								<xs:documentation>use these many rows (default: use all, variables: rows)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="zeros">
				<xs:annotation>
					<xs:documentation>additional zero parameters</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="count" type="expression">
							<xs:annotation>
								<xs:documentation>count of zero parameters (variables: length)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="complement">
				<xs:annotation>
					<xs:documentation>all parameters except those selected</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="parameterSelection" type="parameterSelectorType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>parameter order/selection</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
