									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="removeArcMean" type="boolean" minOccurs="0" default="1"/>
						<xs:element name="startDataFields" type="uint" minOccurs="0" default="0">
