								<xs:documentation>Vector containing the factors</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="NormalsSolverVCE">
				<xs:annotation>
					<xs:documentation>solve normal equations (relative weighting by variance component estimation (VCE))</xs:documentation>
					<xs:appinfo>rename: outputfileNormalequation = outputfileNormalEquation</xs:appinfo>
					<xs:appinfo>rename: normalequation = normalEquation</xs:appinfo>
					<xs:appinfo>tag: NormalEquation</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileSolution" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>parameter vector</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileSigmax" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>standard deviations of the parameters (sqrt of the diagonal of the inverse normal equation)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileCovariance" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>full covariance matrix</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileContribution" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>contribution of normal system components to the solution vector</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileVarianceFactors" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>estimated variance factors as vector</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileNormalEquation" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>the combined normal equation system</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="normalEquation" type="normalEquationType" maxOccurs="unbounded"/>
						<xs:element name="inputfileApproxSolution" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>to accelerate convergence</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="rightHandSideNumberVCE" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>the right hand side number for estimation of variance factors</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="normalsBlockSize" type="uint" minOccurs="0" default="2048">
							<xs:annotation>
								<xs:documentation>block size for distributing the normal equations, 0: one block</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxIterationCount" type="uint" minOccurs="0" default="20">
							<xs:annotation>
								<xs:documentation>maximum number of iterations for variance component estimation</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
