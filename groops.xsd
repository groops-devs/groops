					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Kaula2SigmaPotentialCoefficients">
				<xs:annotation>
					<xs:documentation>Create variances according kaulas rule of thumb</xs:documentation>
					<xs:appinfo>tag: Misc</xs:appinfo>
					<xs:appinfo>tag: PotentialCoefficients</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfilePotentialCoefficients" type="filename"/>
						<xs:element name="minDegree" type="uint" minOccurs="0" default="2"/>
						<xs:element name="maxDegree" type="uint"/>
						<xs:element name="GM" type="double" minOccurs="0" default="3.986004415e+14">
							<xs:annotation>
								<xs:documentation>Geocentric gravitational constant</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378136.3">
							<xs:annotation>
								<xs:documentation>reference radius</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="power" type="double" minOccurs="0" default="2">
							<xs:annotation>
								<xs:documentation>sigma = factor/degree^power</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1e-5">
							<xs:annotation>
