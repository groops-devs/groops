			<xs:element name="InstrumentRemoveEpochsByCriteria">
				<xs:annotation>
					<xs:documentation>Remove epochs which criteria defined through expressions</xs:documentation>
					<xs:appinfo>tag: Instrument</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileInstrument" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>all data is stored in one arc</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileInstrumentRemovedEpochs" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>all data is stored in one arc</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileInstrument" type="filename">
							<xs:annotation>
								<xs:documentation>arcs are concatenated for processing</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileInstrumentReference" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>if given, the reference data is reduced prior to the expressions being evaluated</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="removalCriteria" type="expression" default="abs(data0-data0median) &gt; 3*1.4826*data0mad" maxOccurs="unbounded">
