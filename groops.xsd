					<xs:sequence>
						<xs:element name="inputfileDiagonalMatrix" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Vector with the diagonal elements of the weight matrix</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileBias" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Matrix with right hand sides</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="aprioriSigma" type="double" minOccurs="0" default="1.0"/>
						<xs:element name="startIndex" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>regularization of parameters starts at this index (counting from 0)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="regularizationGeneralized">
				<xs:annotation>
					<xs:documentation>regularization with a sum of partial covariance matrices</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfilePartialCovarianceMatrix" type="filename" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>symmetric matrix (sum of all matrices must be positive definite)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileBiasMatrix" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>bias vector (default: zero vector)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="aprioriSigma" type="double" default="1.0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>apriori sigmas for initial iteration (default: 1.0)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="startIndex" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>regularization of parameters starts at this index (counting from 0)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
