					<xs:appinfo>tag: Orbit</xs:appinfo>
					<xs:appinfo>tag: Covariance</xs:appinfo>
					<xs:appinfo>tag: Instrument</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfile" type="filename"/>
						<xs:element name="inputfileOrbit" type="filename"/>
						<xs:element name="inputfileCovariance" type="filename"/>
						<xs:element name="earthRotation" type="earthRotationType"/>
						<xs:element name="firstLine" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Text for first line</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Orbit2Sp3Format">
				<xs:annotation>
					<xs:documentation>Write orbits (position, velocity, covariance) to SP3 format.</xs:documentation>
					<xs:appinfo>tag: Conversion</xs:appinfo>
					<xs:appinfo>tag: Orbit</xs:appinfo>
					<xs:appinfo>tag: Covariance</xs:appinfo>
					<xs:appinfo>tag: Instrument</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfile" type="filename"/>
						<xs:element name="satellite" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="inputfileOrbit" type="filename"/>
									<xs:element name="inputfileClock" type="filename" minOccurs="0"/>
									<xs:element name="inputfileCovariance" type="filename" minOccurs="0"/>
									<xs:element name="identifier" type="string">
										<xs:annotation>
											<xs:documentation>3 characters (e.g. GNSS PRN: G01)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="orbitAccuracy" type="double" minOccurs="0" default="0">
										<xs:annotation>
											<xs:documentation>[m] used for accuracy codes in header (0 = unknown)</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="earthRotation" type="earthRotationType" minOccurs="0" default="file">
							<xs:annotation>
								<xs:documentation>rotate data into Earth-fixed frame</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gravityfield" type="gravityfieldType" minOccurs="0" maxOccurs="unbounded" default="[{&quot;tides&quot;: {&quot;tides&quot;: {&quot;doodsonHarmonicTide&quot;: {&quot;minDegree&quot;:1, &quot;maxDegree&quot;:1}}}}]">
							<xs:annotation>
								<xs:documentation>degree 1 fluid mantle for CM2CE correction (SP3 orbits should be in center of Earth)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="comment" type="string" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>comment lines (77 char max)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="firstLine" type="string" minOccurs="0" default="RAW   IGb14 FIT  TUG">
							<xs:annotation>
								<xs:documentation>Text for first line e.g:  u+U  IGb14 KIN ITSG</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="writeVelocity" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>write velocity in addition to position</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="useSp3kFormat" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>use the extended sp3k format</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PotentialCoefficients2Icgem">
				<xs:annotation>
					<xs:documentation>write spherical harmonics in ICGEM format</xs:documentation>
					<xs:appinfo>tag: Conversion</xs:appinfo>
					<xs:appinfo>tag: PotentialCoefficients</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfile" type="filename"/>
						<xs:element name="inputfilePotentialCoefficients" type="filename"/>
						<xs:element name="inputfileTrend" type="filename" minOccurs="0"/>
						<xs:element name="oscillation" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="inputfileCosPotentialCoefficients" type="filename"/>
									<xs:element name="inputfileSinPotentialCoefficients" type="filename"/>
									<xs:element name="period" type="string" default="1.0">
										<xs:annotation>
											<xs:documentation>period of oscillation [year]</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="comment" type="string" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>comment in header</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileComment" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>file containing comments for header</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="modelname" type="string">
							<xs:annotation>
								<xs:documentation>name of the model</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="tideSystem" minOccurs="0">
							<xs:annotation>
								<xs:documentation>tide system of model</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element name="zero_tide"/>
									<xs:element name="tide_free"/>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="minDegree" type="uint" minOccurs="0" default="0"/>
						<xs:element name="maxDegree" type="uint" minOccurs="0"/>
						<xs:element name="GM" type="double" minOccurs="0" default="3.986004415e+14">
							<xs:annotation>
								<xs:documentation>Geocentric gravitational constant</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378136.3">
							<xs:annotation>
								<xs:documentation>reference radius</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="time" type="time" minOccurs="0">
							<xs:annotation>
								<xs:documentation>reference time</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PsmslOceanBottomPressure2TimeSeries">
				<xs:annotation>
					<xs:documentation>read ocean bottom pressure (OBP) time series in the PSMSL file format</xs:documentation>
					<xs:appinfo>tag: Conversion</xs:appinfo>
				</xs:annotation>
