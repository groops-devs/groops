									<xs:element name="throwException">
										<xs:annotation>
											<xs:documentation>throw exception if no matching pattern is found</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="inputfileReceiverDefinition" type="filename" minOccurs="0" default="{groopsDataDir}/gnss/receiverLowEarthOrbiter/">
							<xs:annotation>
								<xs:documentation>observed signal types</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileAccuracyDefinition" type="filename" default="{groopsDataDir}/gnss/receiverLowEarthOrbiter/">
							<xs:annotation>
								<xs:documentation>elevation and azimut dependent accuracy</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileObservations" type="filename" minOccurs="0" default="gnssReceiver_{loopTime:%D}.dat"/>
						<xs:element name="inputfileOrbit" type="filename">
							<xs:annotation>
								<xs:documentation>approximate positions</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileStarCamera" type="filename">
							<xs:annotation>
								<xs:documentation>satellite attitude</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="sigmaFactorPhase" type="expression" minOccurs="0">
							<xs:annotation>
								<xs:documentation>PHASE: factor = f(FREQ, ELE, SNR, ROTI, dTEc, IONOINDEX)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="sigmaFactorCode" type="expression" minOccurs="0">
							<xs:annotation>
								<xs:documentation>CODE: factor = f(FREQ, ELE, SNR, ROTI, dTEc, IONOINDEX)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="supportsIntegerAmbiguities" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>receiver tracks full cycle integer ambiguities</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="wavelengthFactor" type="double" minOccurs="0" default="1.">
							<xs:annotation>
								<xs:documentation>factor to account for half-wavelength observations (collected by codeless squaring techniques)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="useType" type="gnssType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>only use observations that match any of these patterns</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ignoreType" type="gnssType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>ignore observations that match any of these patterns</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="elevationCutOff" type="angle" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>[degree] ignore observations below cutoff</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="minObsCountPerTrack" type="uint" minOccurs="0" default="20">
							<xs:annotation>
								<xs:documentation>tracks with less number of epochs with observations are dropped</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="preprocessing">
							<xs:annotation>
								<xs:documentation>settings for preprocessing of observations/stations</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="printStatistics" type="boolean" minOccurs="0" default="1">
										<xs:annotation>
											<xs:documentation>print preprocesssing statistics for all receivers</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="huber" type="double" minOccurs="0" default="2.5">
										<xs:annotation>
											<xs:documentation>residuals &gt; huber*sigma0 are downweighted</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="huberPower" type="double" minOccurs="0" default="1.5">
										<xs:annotation>
											<xs:documentation>residuals &gt; huber: sigma=(e/huber)^huberPower*sigma0</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="codeMaxPositionDiff" type="double" minOccurs="0" default="100">
										<xs:annotation>
											<xs:documentation>[m] max. allowed position error by PPP code only clock error estimation</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="denoisingLambda" type="double" minOccurs="0" default="5">
										<xs:annotation>
											<xs:documentation>regularization parameter for total variation denoising used in cylce slip detection</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="tecWindowSize" type="uint" minOccurs="0" default="15">
										<xs:annotation>
											<xs:documentation>(0 = disabled) window size for TEC smoothness evaluation used in cycle slip detection</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="tecSigmaFactor" type="double" minOccurs="0" default="3.5">
										<xs:annotation>
											<xs:documentation>factor applied to moving standard deviation used as threshold in TEC smoothness evaluation during cycle slip detection</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="outputfileTrackBefore" type="filename" minOccurs="0">
										<xs:annotation>
											<xs:documentation>variables {station}, {prn}, {timeStart}, {timeEnd}, {types}, TEC and MW-like combinations in cycles for each track before cycle slip detection</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="outputfileTrackAfter" type="filename" minOccurs="0">
										<xs:annotation>
											<xs:documentation>variables {station}, {prn}, {timeStart}, {timeEnd}, {types}, TEC and MW-like combinations in cycles for each track after cycle slip detection</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="gnssTransmitterGeneratorType">
