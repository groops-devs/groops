			<xs:element name="InstrumentArcCalculate">
				<xs:annotation>
					<xs:documentation>Instrument data calculation arc wise.</xs:documentation>
					<xs:appinfo>tag: Instrument</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileInstrument" type="filename" minOccurs="0"/>
						<xs:element name="inputfileInstrument" type="filename" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>data columns are appended to the right</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="constant" type="expression" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>define a constant by name=value</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="parameter" type="expression" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>define a parameter by name[=value]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="leastSquares" type="expression" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>try to minimize the expression by adjustment of the parameters</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="removalCriteria" type="expression" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>row is removed if one criterion evaluates true.</xs:documentation>
