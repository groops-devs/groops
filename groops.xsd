			<xs:element name="inputfileAccelerometer1" type="filename" minOccurs="0">
				<xs:annotation>
					<xs:documentation>non-gravitational forces in satellite reference frame A</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inputfileAccelerometer2" type="filename" minOccurs="0">
				<xs:annotation>
					<xs:documentation>non-gravitational forces in satellite reference frame B</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="forces" type="forcesType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="thermosphereType">
