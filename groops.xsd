	<xs:complexType name="noiseGeneratorType">
		<xs:annotation>
			<xs:documentation>generate different types of noise</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="white">
				<xs:annotation>
					<xs:documentation>white noise</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="sigma" type="double" default="1.0">
							<xs:annotation>
								<xs:documentation>standard deviation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="initRandom" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>start value for pseudo random sequence, 0: real random</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="expressionPSD">
				<xs:annotation>
					<xs:documentation>defined by one sided PSD</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="noise" type="noiseGeneratorType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Basis noise</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="psd" type="expression" default="1">
							<xs:annotation>
								<xs:documentation>one sided PSD (variable: freq [Hz]) [unit^2/Hz]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="sampling" type="double" default="1">
							<xs:annotation>
								<xs:documentation>to determine frequency [seconds]</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="filter">
				<xs:annotation>
					<xs:documentation>noise conforms to psd of filter</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="filter" type="digitalFilterType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>digital filter</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="noise" type="noiseGeneratorType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Basis noise</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="warmupEpochCount" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>number of additional epochs at before start and after end</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="overSamplingFactor" type="uint" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>noise with multiple higher sampling -&gt; filter -&gt; decimate</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="powerLaw">
				<xs:annotation>
					<xs:documentation>noise follows power law relationship (f^alpha)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="noise" type="noiseGeneratorType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Basis noise</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="alpha" type="double" default="0">
							<xs:annotation>
								<xs:documentation>Exponent of the power law relationship 1/f^alpha</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="normalEquationType">
