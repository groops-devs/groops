	</xs:complexType>
	<xs:complexType name="tidesType">
		<xs:annotation>
			<xs:documentation>tidal forces</xs:documentation>
			<xs:appinfo>rename: poleTide2010 = poleTide</xs:appinfo>
			<xs:appinfo>rename: poleOceanTide2010 = oceanPoleTide</xs:appinfo>
			<xs:appinfo>rename: moonTide = solidMoonTide</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="astronomicalTide">
				<xs:annotation>
					<xs:documentation>direct tides from sun, moon and planets</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="useMoon" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>TGP of moon</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="useSun" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>TGP of sun</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="usePlanets" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>TGP of planets</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="useEarth" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>TGP of Earth</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="c20Earth" type="double" minOccurs="0" default="-4.84166854896119e-04">
							<xs:annotation>
								<xs:documentation>J2 flattening of the Earth</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="earthTide">
				<xs:annotation>
					<xs:documentation>solid earth tides</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileEarthtide" type="filename" default="{groopsDataDir}/tides/earthAnelastic2003.xml"/>
						<xs:element name="includePermanentTide" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>results in FALSE: zero tide, TRUE: tide free gravity field</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="doodsonHarmonicTide">
				<xs:annotation>
					<xs:documentation>tides with harmonic representation, e.g. ocean tides</xs:documentation>
					<xs:appinfo>rename: inputfileOcean = inputfileTides</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileTides" type="filename" default="{groopsDataDir}/tides/oceanTide_fes2014b_n180_version20170520.dat"/>
						<xs:element name="inputfileAdmittance" type="filename" minOccurs="0" default="{groopsDataDir}/tides/oceanTide_fes2014b_admittance_linear_linear.txt">
							<xs:annotation>
								<xs:documentation>interpolation of minor constituents</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="selectDoodson" type="doodson" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>consider only these constituents, code number (e.g. 255.555) or darwin name (e.g. M2)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="minDegree" type="uint" minOccurs="0" default="2"/>
						<xs:element name="maxDegree" type="uint" minOccurs="0"/>
						<xs:element name="nodeCorr" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>nodal corrections: 0-no corr, 1-IHO, 2-Schureman</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="poleTide">
				<xs:annotation>
					<xs:documentation>centrifugal effect of polar motion</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="scale" type="double" minOccurs="0" default="1.333e-9"/>
						<xs:element name="outPhase" type="double" minOccurs="0" default="0.0115"/>
						<xs:element name="inputfileMeanPole" type="filename" default="{groopsDataDir}/tides/secularPole2018.xml"/>
						<xs:element name="horizontalDisplacement" type="double" minOccurs="0" default="0.009">
							<xs:annotation>
								<xs:documentation>[m]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="verticalDisplacement" type="double" minOccurs="0" default="0.033">
							<xs:annotation>
								<xs:documentation>[m]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="oceanPoleTide">
				<xs:annotation>
					<xs:documentation>The ocean pole tide is generated by the centrifugal effect of polar motion on the oceans</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileOceanPole" type="filename" default="{groopsDataDir}/tides/oceanPoleTide_desai2004.dat"/>
						<xs:element name="minDegree" type="uint" minOccurs="0" default="2"/>
						<xs:element name="maxDegree" type="uint" minOccurs="0"/>
						<xs:element name="gammaReal" type="double" minOccurs="0" default="0.6870"/>
						<xs:element name="gammaImaginary" type="double" minOccurs="0" default="0.0036"/>
						<xs:element name="inputfileMeanPole" type="filename" default="{groopsDataDir}/tides/secularPole2018.xml"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="centrifugal">
				<xs:annotation>
					<xs:documentation>Current centrifugal force from Earth rotation</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="solidMoonTide">
				<xs:annotation>
					<xs:documentation>solid moon tides (at moon)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="k20" type="double" minOccurs="0" default="0.0213"/>
						<xs:element name="k30" type="double" minOccurs="0" default="0.0"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="group">
				<xs:annotation>
					<xs:documentation>group tides</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="tides" type="tidesType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="timeSeriesType">
		<xs:annotation>
			<xs:documentation>Generates time series</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="uniformSampling">
				<xs:annotation>
					<xs:documentation>uniform series with given sampling</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="timeStart" type="time">
							<xs:annotation>
								<xs:documentation>first point in time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeEnd" type="time">
							<xs:annotation>
								<xs:documentation>last point in time will be less or equal timeEnd</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="sampling" type="time">
							<xs:annotation>
								<xs:documentation>time step between points in time</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="uniformInterval">
				<xs:annotation>
					<xs:documentation>uniform series with given interval count</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="timeStart" type="time">
							<xs:annotation>
								<xs:documentation>1st point of the time series</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeEnd" type="time">
							<xs:annotation>
								<xs:documentation>last point of the time series</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="intervalCount" type="uint">
							<xs:annotation>
								<xs:documentation>count of intervals, count+1 points in time will generated</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="irregular">
				<xs:annotation>
					<xs:documentation>every single point in time is given</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="time" type="time" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>explicit list of points in time</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="monthly">
				<xs:annotation>
					<xs:documentation>time series based on months</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="monthStart" type="uint"/>
						<xs:element name="yearStart" type="uint"/>
						<xs:element name="monthEnd" type="uint"/>
						<xs:element name="yearEnd" type="uint"/>
						<xs:element name="useMonthMiddle" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>time points are mid of months, otherwise the 1st of each month + a time point behind the last month</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="yearly">
				<xs:annotation>
					<xs:documentation>time series based on years</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="yearStart" type="uint"/>
						<xs:element name="yearEnd" type="uint"/>
						<xs:element name="useYearMiddle" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>time points are mid of years, otherwise the 1st of each year + a time point behind the last year</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="everyMonth">
				<xs:annotation>
					<xs:documentation>time series based on months</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="timeStart" type="time">
							<xs:annotation>
								<xs:documentation>first point in time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeEnd" type="time">
							<xs:annotation>
								<xs:documentation>last point in time will be less or equal timeEnd</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="everyYear">
				<xs:annotation>
					<xs:documentation>time series based on years</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="timeStart" type="time">
							<xs:annotation>
								<xs:documentation>first point in time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeEnd" type="time">
							<xs:annotation>
								<xs:documentation>last point in time will be less or equal timeEnd</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="instrument">
				<xs:annotation>
					<xs:documentation>read time series from an instrument file</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileInstrument" type="filename"/>
						<xs:element name="timeStart" type="time" minOccurs="0">
							<xs:annotation>
								<xs:documentation>exclude peochs before this epoch</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeEnd" type="time" minOccurs="0">
							<xs:annotation>
								<xs:documentation>only epochs before this time are used</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="instrumentArcIntervals">
				<xs:annotation>
					<xs:documentation>try to reproduce arc interval time series from an instrument file</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileInstrument" type="filename">
							<xs:annotation>
								<xs:documentation>Must be regular. Time series is first epoch of each arc plus one time step extrapolated from last epoch of last arc.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="revolutions">
				<xs:annotation>
					<xs:documentation>create time series from orbit file (revolutions)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileOrbit" type="filename"/>
						<xs:element name="timeStart" type="time" minOccurs="0">
							<xs:annotation>
								<xs:documentation>exclude peochs before this epoch</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeEnd" type="time" minOccurs="0">
							<xs:annotation>
								<xs:documentation>only epochs before this time are used</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="exclude">
				<xs:annotation>
					<xs:documentation>exclude times from a given time series</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="timeSeries" type="timeSeriesType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>time series to be created</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeSeriesExclude" type="timeSeriesType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>exclude this time points from time series (within margin)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="excludeMargin" type="double" minOccurs="0" default="1e-5">
							<xs:annotation>
								<xs:documentation>on both sides [seconds]</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="conditional">
				<xs:annotation>
					<xs:documentation>time series based depending on conditions</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="timeSeries" type="timeSeriesType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>only times for which condition is met will be included</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopTime" type="string" minOccurs="0" default="loopTime">
							<xs:annotation>
								<xs:documentation>variable with time of each loop</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="condition" type="conditionType">
							<xs:annotation>
								<xs:documentation>test for each time</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="interpolate">
				<xs:annotation>
					<xs:documentation>interpolate between created times</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="timeSeries" type="timeSeriesType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>time series to be created</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="nodeInterpolation" type="uint" default="1">
							<xs:annotation>
								<xs:documentation>interpolates count points in each time interval given by the time series</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="troposphereType">
		<xs:annotation>
			<xs:documentation>signal delay in the atmosphere</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="viennaMapping">
				<xs:annotation>
					<xs:documentation>Vienna Mapping Function</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileVmfCoefficients" type="filename" default="{groopsDataDir}/troposphere/" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>ah, aw, zhd, zwd coefficients</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileGpt" type="filename" default="{groopsDataDir}/troposphere/gpt3_grid1deg.dat">
							<xs:annotation>
								<xs:documentation>gridded GPT data</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="aHeight" type="double" minOccurs="0" default="2.53e-5">
							<xs:annotation>
								<xs:documentation>parameter a (height correction)</xs:documentation>
							</xs:annotation>
						</xs:element>
