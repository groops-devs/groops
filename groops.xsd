					<xs:documentation>generic x/y axis</xs:documentation>
					<xs:appinfo>rename: annotation = majorTickSpacing</xs:appinfo>
					<xs:appinfo>rename: frame = minorTickSpacing</xs:appinfo>
					<xs:appinfo>rename: grid = gridLineSpacing</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="min" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The minimum value of the axis. If no value is given, the minimum scale value is set automatically.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="max" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The maximum value of the axis. If no value is given, the maximum scale value is set automatically.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="majorTickSpacing" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The boundary annotation.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="minorTickSpacing" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The spacing of the frame tick intervals.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gridLineSpacing" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The spacing of the grid line intervals</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gridLine" type="plotLineType" minOccurs="0" default="{&quot;solid&quot;: {&quot;width&quot;:&quot;0.25&quot;, &quot;color&quot;:&quot;gray&quot;}}">
							<xs:annotation>
								<xs:documentation>The style of the grid lines.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="unit" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Naming unit to append to the axis values.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="label" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The description of the axis.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="logarithmic" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>If set to &apos;yes&apos;, a logarithmic scale is used for the axis.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="color" type="plotColorType">
							<xs:annotation>
								<xs:documentation>Setting the color of the axis bars and labels.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="changeDirection" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>If set to &apos;yes&apos;, the directions right/up are changed to left/down.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="time">
				<xs:annotation>
					<xs:documentation>intepret x-values as MJD</xs:documentation>
					<xs:appinfo>rename: annotation = majorTickSpacing</xs:appinfo>
					<xs:appinfo>rename: frame = minorTickSpacing</xs:appinfo>
					<xs:appinfo>rename: grid = gridLineSpacing</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="min" type="time" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The minimum value of the time axis. If no value is given, the minimum scale value is set automatically.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="max" type="time" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The maximum value of the time axis. If no value is given, the maximum scale value is set automatically.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="majorTickSpacing" type="string" minOccurs="0" default="2o">
							<xs:annotation>
								<xs:documentation>Y: year, o: month</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="minorTickSpacing" type="string" minOccurs="0" default="1o">
							<xs:annotation>
								<xs:documentation>D: date, d: day</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gridLineSpacing" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>H: clock, h: hour, m: minute, s: second</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="secondary" minOccurs="0">
							<xs:annotation>
								<xs:documentation>secondary time axis</xs:documentation>
								<xs:appinfo>rename: annotation = majorTickSpacing</xs:appinfo>
								<xs:appinfo>rename: frame = minorTickSpacing</xs:appinfo>
								<xs:appinfo>rename: grid = gridLineSpacing</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="majorTickSpacing" type="string" minOccurs="0" default="1Y">
										<xs:annotation>
											<xs:documentation>Y: year, o: month</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="minorTickSpacing" type="string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>D: date, d: day</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="gridLineSpacing" type="string" minOccurs="0" default="1Y">
										<xs:annotation>
											<xs:documentation>H: clock, h: hour, m: minute, s: second</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="color" type="plotColorType">
							<xs:annotation>
								<xs:documentation>color of axis bars and labels</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gridLine" type="plotLineType" minOccurs="0" default="{&quot;solid&quot;: {&quot;width&quot;:&quot;0.25&quot;, &quot;color&quot;:&quot;gray&quot;}}">
							<xs:annotation>
								<xs:documentation>The style of the grid lines.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="changeDirection" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>right-&gt;left / up-&gt;down</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="options" type="string" minOccurs="0" default="[&quot;FORMAT_DATE_MAP=yyyy-mm-dd&quot;, &quot;FORMAT_CLOCK_MAP=hh:mm&quot;]" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>adjust date format</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="labeled">
				<xs:annotation>
					<xs:documentation>major ticks with string labels</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="labels" type="string" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>tick labels (ticks are placed at their index. e.g. 0, 1, ..., 5)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="min" type="expression" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>minimum value of the axis</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="max" type="expression" minOccurs="0" default="labelCount-1">
							<xs:annotation>
								<xs:documentation>maximum values of the axis</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="orthogonalLabels" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>labels are oriented orthogonal to axis</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gridLine" type="plotLineType" minOccurs="0" default="{&quot;solid&quot;: {&quot;width&quot;:&quot;0.25&quot;, &quot;color&quot;:&quot;gray&quot;}}">
							<xs:annotation>
								<xs:documentation>The style of the grid lines.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="color" type="plotColorType">
							<xs:annotation>
								<xs:documentation>set the color of the axis and labels</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="changeDirection" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>If set to &apos;yes&apos;, the directions right/up are changed to left/down.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
