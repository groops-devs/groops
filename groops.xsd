								<xs:documentation>start</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="countDataFields" type="uint" minOccurs="0">
							<xs:annotation>
								<xs:documentation>number of data fields (default: all)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="perColumn" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>compute statistic per column</xs:documentation>
							</xs:annotation>
