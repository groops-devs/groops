					<xs:documentation>draw rectangle</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="minX" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>empty: left</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxX" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>empty: right</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="minY" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>empty: bottom</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxY" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>empty: top</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="description" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>text of the legend</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="edgeLine" type="plotLineType" minOccurs="0"/>
						<xs:element name="fillColor" type="plotColorType" minOccurs="0"/>
						<xs:element name="plotOnSecondAxis" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>draw dataset on a second Y-axis (if available).</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="text">
				<xs:annotation>
					<xs:documentation>text</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="text" type="string"/>
						<xs:element name="originX" type="double"/>
						<xs:element name="originY" type="double"/>
						<xs:element name="offsetX" type="double" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>[cm] x-offset from origin</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="offsetY" type="double" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>[cm] y-offset from origin</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="alignment" type="string" minOccurs="0" default="BL">
							<xs:annotation>
								<xs:documentation>L, C, R (left, center, right) and T, M, B (top, middle, bottom)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="fontSize" type="double" minOccurs="0" default="10">
							<xs:annotation>
								<xs:documentation>[pt]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="fontColor" type="plotColorType"/>
						<xs:element name="clip" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>clip at boundaries</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="plotOnSecondAxis" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>draw dataset on a second Y-axis (if available).</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="degreeAmplitudes">
				<xs:annotation>
					<xs:documentation>degree amplitudes of a gravity field</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileMatrix" type="filename">
							<xs:annotation>
								<xs:documentation>degree amplitudes</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="valueDegree" type="expression" minOccurs="0" default="data0">
							<xs:annotation>
								<xs:documentation>expression for x-values (degrees) (input columns are named data0, data1, ...)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="valueSignal" type="expression" minOccurs="0" default="data1">
							<xs:annotation>
								<xs:documentation>expression for y-values (signal) (input columns are named data0, data1, ...)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="valueErrors" type="expression" minOccurs="0" default="data2">
							<xs:annotation>
								<xs:documentation>expression for y-values (formal errors)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="description" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>text of the legend</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="lineSignal" type="plotLineType" minOccurs="0" default="solid"/>
						<xs:element name="lineErrors" type="plotLineType" minOccurs="0" default="{&quot;custom&quot;: {&quot;style&quot;:&quot;5_2:0&quot;}}"/>
						<xs:element name="plotOnSecondAxis" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>draw dataset on a second Y-axis (if available).</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="plotLegendType">
		<xs:sequence>
			<xs:element name="width" type="double" minOccurs="0" default="10">
				<xs:annotation>
					<xs:documentation>legend width [cm]</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="height" type="double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>legend height [cm] (default: estimated)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="positionX" type="double" minOccurs="0" default="1.05">
				<xs:annotation>
					<xs:documentation>legend x-position in normalized (0-1) coordinates.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="positionY" type="double" minOccurs="0" default="1.0">
				<xs:annotation>
					<xs:documentation>legend y-position in normalized (0-1) coordinates.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="anchorPoint" type="string" minOccurs="0" default="TL">
				<xs:annotation>
					<xs:documentation>Two character combination of L, C, R (for left, center, or right) and T, M, B for top, middle, or bottom. e.g., TL for top left</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="columns" type="uint" minOccurs="0" default="1">
				<xs:annotation>
					<xs:documentation>number of columns in legend</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="textColor" type="plotColorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>color of the legend text</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fillColor" type="plotColorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>fill color of the legend box</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="edgeLine" type="plotLineType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>style of the legend box edge</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="plotLineType">
		<xs:annotation>
			<xs:documentation>line style</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="solid">
				<xs:annotation>
					<xs:documentation>solid line</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="width" type="double" minOccurs="0" default="1.5">
							<xs:annotation>
								<xs:documentation>line width [p]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="color" type="plotColorType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="dashed">
				<xs:annotation>
					<xs:documentation>dashed line</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="width" type="double" minOccurs="0" default="1.5">
							<xs:annotation>
								<xs:documentation>line width [p]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="color" type="plotColorType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="dotted">
				<xs:annotation>
					<xs:documentation>dotted line</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="width" type="double" minOccurs="0" default="1.5">
							<xs:annotation>
								<xs:documentation>line width [p]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="color" type="plotColorType">
							<xs:annotation>
								<xs:documentation>  </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="custom">
				<xs:annotation>
					<xs:documentation>custom line (e.g. dash-dot)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="style" type="string" default=".-">
							<xs:annotation>
								<xs:documentation>line style code</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="width" type="double" minOccurs="0" default="1.5">
							<xs:annotation>
								<xs:documentation>line width [p]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="color" type="plotColorType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="plotMapLayerType">
		<xs:annotation>
			<xs:documentation>plot layers</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="griddedData">
				<xs:annotation>
					<xs:documentation>data with regular sampling</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileGriddedData" type="filename"/>
						<xs:element name="value" type="expression" default="data0">
							<xs:annotation>
								<xs:documentation>expression to compute values (input columns are named data0, data1, ...)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="increment" type="angle" minOccurs="0">
							<xs:annotation>
								<xs:documentation>the grid spacing [degrees]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="illuminate" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>illuminate grid</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="resample" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="intermediateDpi" type="double" minOccurs="0" default="100">
										<xs:annotation>
											<xs:documentation>oversample grid for a smoother visual effect</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="interpolationMethod">
										<xs:annotation>
											<xs:documentation>interpolation method for oversampling</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:choice>
												<xs:element name="bspline">
													<xs:annotation>
														<xs:documentation>B-Spline interpolation</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="bicubic">
													<xs:annotation>
														<xs:documentation>bicubic interpolation</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="bilinear">
													<xs:annotation>
														<xs:documentation>bilinear interpolation</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="nearest">
													<xs:annotation>
														<xs:documentation>nearest neighbour interpolation</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:choice>
										</xs:complexType>
									</xs:element>
									<xs:element name="threshold" type="double" minOccurs="0" default="0.5">
										<xs:annotation>
											<xs:documentation>A threshold of 1.0 requires all (4 or 16) nodes involved in interpolation to be non-NaN. 0.5 will interpolate about half way from a non-NaN value; 0.1 will go about 90% of the way.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="gridlineRegistered" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>treat input as point values instead of cell means</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="points">
				<xs:annotation>
					<xs:documentation>colored points</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileGriddedData" type="filename"/>
						<xs:element name="value" type="expression" minOccurs="0" default="data0">
							<xs:annotation>
								<xs:documentation>expression to compute color (input columns are named data0, data1, ...)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="symbol" type="plotSymbolType" minOccurs="0" default="1"/>
						<xs:element name="line" type="plotLineType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>style of connecting lines</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="drawLineAsGreatCircle" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>draw connecting lines as great circles (otherwise, a straight line is drawn instead)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="arrows">
				<xs:annotation>
					<xs:documentation>colored arrows</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileGriddedData" type="filename">
							<xs:annotation>
								<xs:documentation>grid file with north and east values for arrows</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="valueNorth" type="expression" default="data0">
							<xs:annotation>
								<xs:documentation>expression to compute north values (input columns are named data0, data1, ...)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="valueEast" type="expression" default="data1">
							<xs:annotation>
								<xs:documentation>expression to compute east values (input columns are named data0, data1, ...)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="value" type="expression" minOccurs="0">
							<xs:annotation>
								<xs:documentation>expression to compute arrow color (input columns are named data0, data1, ...)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="scale" type="double" minOccurs="0" default="50">
							<xs:annotation>
								<xs:documentation>[cm per input unit] length scale factor</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="penSize" type="double" default="1">
							<xs:annotation>
								<xs:documentation>[pt] width of arrow shaft</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="headSize" type="double" default="5">
							<xs:annotation>
								<xs:documentation>[pt] size of arrow head, 0: no head, negative: reverse head</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="color" type="plotColorType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>empty: from value</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="scaleArrow" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>draw an arrow for scale reference</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="originX" type="double" default="0.055">
										<xs:annotation>
											<xs:documentation>[0-1] 0: left, 1: right</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="originY" type="double" default="0.065">
										<xs:annotation>
											<xs:documentation>[0-1] 0: bottom, 1: top</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="length" type="double">
										<xs:annotation>
											<xs:documentation>in same unit as valueNorth and valueEast</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="unit" type="string">
										<xs:annotation>
											<xs:documentation>displayed unit text (e.g. 1 cm)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="label" type="string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>description of the arrows</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="polygon">
				<xs:annotation>
					<xs:documentation>boundaries</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfilePolygon" type="filename" default="{groopsDataDir}/border/"/>
						<xs:element name="line" type="plotLineType" minOccurs="0" default="solid">
							<xs:annotation>
								<xs:documentation>style of border lines</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="fillColor" type="plotColorType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>polygon fill color (no fill color: determine from value if given, else: no fill)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="value" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>value to compute fill color from a colorbar (ignored if a fillColor is given)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="drawLineAsGreatCircle" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>draw connecting lines as great circles (otherwise, a straight line is drawn instead)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="coast">
				<xs:annotation>
					<xs:documentation>coast lines, ...</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="resolution" default="medium">
							<xs:complexType>
								<xs:choice>
									<xs:element name="crude"/>
									<xs:element name="low"/>
									<xs:element name="medium"/>
									<xs:element name="high"/>
									<xs:element name="full"/>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="line" type="plotLineType" minOccurs="0" default="{&quot;solid&quot;: {&quot;width&quot;: &quot;1&quot;}}">
							<xs:annotation>
								<xs:documentation>line style for coastlines</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="landColor" type="plotColorType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>fill land area</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="oceanColor" type="plotColorType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>fill ocean area</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="minArea" type="uint" minOccurs="0" default="5000">
							<xs:annotation>
								<xs:documentation>[km^2] features with a smaller area than this are dropped</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="rivers">
				<xs:annotation>
					<xs:documentation>major rivers</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="class">
							<xs:complexType>
