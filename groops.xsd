	<xs:complexType name="gnssProcessingStepType">
		<xs:choice>
			<xs:element name="estimate">
				<xs:annotation>
					<xs:documentation>least squares adjustment</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="computeResiduals" type="boolean" minOccurs="0" default="1"/>
						<xs:element name="adjustSigma0" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>adjust sigma0 by scale factor (per receiver and type)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="computeWeights" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>downweight outliers</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="huber" type="double" minOccurs="0" default="2.5">
							<xs:annotation>
								<xs:documentation>residuals &gt; huber*sigma0 are downweighted</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="huberPower" type="double" minOccurs="0" default="1.5">
							<xs:annotation>
								<xs:documentation>residuals &gt; huber: sigma=(e/huber)^huberPower*sigma0</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="convergenceThreshold" type="double" minOccurs="0" default="0.01">
							<xs:annotation>
								<xs:documentation>[m] stop iteration once full convergence is reached</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxIterationCount" type="uint" minOccurs="0" default="3">
							<xs:annotation>
								<xs:documentation>maximum number of iterations</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="resolveAmbiguities">
				<xs:annotation>
					<xs:documentation>resolve integer ambiguities</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileAmbiguities" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>resolved ambiguities</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="sigmaMaxResolve" type="double" minOccurs="0" default="0.2">
							<xs:annotation>
								<xs:documentation>max. allowed std. dev. of ambiguity to resolve [cycles]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="searchBlockSize" type="uint" minOccurs="0" default="200">
							<xs:annotation>
								<xs:documentation>block size for blocked integer search</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxSearchSteps" type="uint" minOccurs="0" default="200000000">
							<xs:annotation>
								<xs:documentation>max. steps of integer search for each block</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="incompleteAction" default="shrinkBlockSize">
							<xs:annotation>
								<xs:documentation>if not all solutions tested after maxSearchSteps</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element name="stop">
										<xs:annotation>
											<xs:documentation>stop searching, ambiguities remain float in this block</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="resolve">
										<xs:annotation>
											<xs:documentation>use best integer solution found so far</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="shrinkBlockSize">
										<xs:annotation>
											<xs:documentation>try again with half block size</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="throwException">
										<xs:annotation>
											<xs:documentation>stop and throw an exception</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="computeResiduals" type="boolean" minOccurs="0" default="1"/>
						<xs:element name="adjustSigma0" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>adjust sigma0 by scale factor (per receiver and type)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="computeWeights" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>downweight outliers</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="huber" type="double" minOccurs="0" default="2.5">
							<xs:annotation>
								<xs:documentation>residuals &gt; huber*sigma0 are downweighted</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="huberPower" type="double" minOccurs="0" default="1.5">
							<xs:annotation>
								<xs:documentation>residuals &gt; huber: sigma=(e/huber)^huberPower*sigma0</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="computeCovarianceMatrix">
				<xs:annotation>
					<xs:documentation>compute covariance matrix</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="writeResults">
