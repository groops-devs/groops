									<xs:element name="inputfileGriddedData" type="filename">
										<xs:annotation>
											<xs:documentation>station positions with displacement data</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="observationNorth" type="expression" default="data0">
										<xs:annotation>
											<xs:documentation>displacement [m]</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="observationEast" type="expression" default="data1">
										<xs:annotation>
											<xs:documentation>displacement [m]</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="observationUp" type="expression" default="data2">
										<xs:annotation>
											<xs:documentation>displacement [m]</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="sigmaNorth" type="expression" minOccurs="0" default="data3">
										<xs:annotation>
											<xs:documentation>accuracy, 1/sigma used as weighting</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="sigmaEast" type="expression" minOccurs="0" default="data4">
										<xs:annotation>
											<xs:documentation>accuracy, 1/sigma used as weighting</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="sigmaUp" type="expression" minOccurs="0" default="data5">
										<xs:annotation>
											<xs:documentation>accuracy, 1/sigma used as weighting</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="inGlobalFrame" type="boolean" minOccurs="0" default="0">
										<xs:annotation>
											<xs:documentation>obs/sigmas given in global x,y,z frame instead of north,east,up</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="referencefield" type="gravityfieldType" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="time" type="time" minOccurs="0">
							<xs:annotation>
								<xs:documentation>for reference field and parametrization</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="parametrizationGravity" type="parametrizationGravityType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>of loading (+defo) potential</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="estimateTranslation" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>coordinate center</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="estimateScale" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>scale factor of position</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="estimateRotation" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>rotation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileDeformationLoadLoveNumber" type="filename" default="{groopsDataDir}/loading/deformationLoveNumbers_CM_Gegout97.txt"/>
						<xs:element name="inputfilePotentialLoadLoveNumber" type="filename" minOccurs="0" default="{groopsDataDir}/loading/loadLoveNumbers_Gegout97.txt">
							<xs:annotation>
								<xs:documentation>if full potential is given and not only loading potential</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378137.0">
							<xs:annotation>
								<xs:documentation>reference radius for ellipsoid</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inverseFlattening" type="double" minOccurs="0" default="298.2572221010">
							<xs:annotation>
								<xs:documentation>reference flattening for ellipsoid, 0: sphere</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="orbitPropagatorType">
		<xs:annotation>
			<xs:documentation>algorithm for integration</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="euler">
				<xs:annotation>
					<xs:documentation>Euler step method</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rungeKutta4">
				<xs:annotation>
					<xs:documentation>The classical Runge-Kutta 4 method</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="adamsBashforthMoulton">
				<xs:annotation>
					<xs:documentation>Adams-Bashforth-Moulton class of predictor-corrector method</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="order" type="uint" default="4">
							<xs:annotation>
								<xs:documentation>Order of the Adams-Bashforth type propagator.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="applyMoultonCorrector" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>Corrector step after Adams-Bashforth predcition.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="warmup" type="orbitPropagatorType" default="rungeKutta4"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="stoermerCowell">
				<xs:annotation>
					<xs:documentation>Stoermer-Cowell predictor-corrector method</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="order" type="uint" default="4">
							<xs:annotation>
								<xs:documentation>Order of the Stoermer-Cowell type propagator.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="warmup" type="orbitPropagatorType" default="rungeKutta4"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="gaussJackson">
				<xs:annotation>
					<xs:documentation>Gauss-Jackson method</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="order" type="uint" default="8">
							<xs:annotation>
								<xs:documentation>of Gauss-Jackson method.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="warmup" type="orbitPropagatorType" default="rungeKutta4"/>
						<xs:element name="correctorIterations" type="uint" minOccurs="0" default="10">
							<xs:annotation>
								<xs:documentation>Maximum number of iterations to run the corrector step for.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="epsilon" type="double" minOccurs="0" default="1e-15">
							<xs:annotation>
								<xs:documentation>Convergence criteria for position, velocity, and acceleration tests.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="polynomial">
				<xs:annotation>
					<xs:documentation>Integration Polynomial method using prediction only</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="degree" type="uint" default="9">
							<xs:annotation>
								<xs:documentation>polynomial degree to integrate accelerations</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="shift" type="int" default="2">
							<xs:annotation>
								<xs:documentation>shift polynomial in future (predicted accelerations)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="epsilon" type="double" default="1e-6">
							<xs:annotation>
								<xs:documentation>[m] max. position change to recompute forces</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="warmup" type="orbitPropagatorType" default="rungeKutta4">
							<xs:annotation>
								<xs:documentation>to compute epochs before start epoch</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="corrector" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>apply corrector iteration if position change is larger than epsilon</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="file">
				<xs:annotation>
					<xs:documentation>Read orbit from file</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileOrbit" type="filename">
							<xs:annotation>
								<xs:documentation>epoch at timeStart is not used</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="margin" type="double" default="1e-5">
							<xs:annotation>
								<xs:documentation>[seconds] to find identical times</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="recomputeForces" type="boolean" minOccurs="0" default="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="parameterNamesType">
		<xs:annotation>
			<xs:documentation>generate a list of parameter names</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="name">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="object" type="string" minOccurs="0">
