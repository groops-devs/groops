								<xs:documentation>ellipsoidal height expression (variables &apos;height&apos;, &apos;L&apos;, &apos;B&apos;)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378137.0">
							<xs:annotation>
								<xs:documentation>major axsis of the ellipsoid/sphere</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inverseFlattening" type="double" minOccurs="0" default="298.2572221010">
							<xs:annotation>
								<xs:documentation>flattening of the ellipsoid, 0: sphere</xs:documentation>
							</xs:annotation>
						</xs:element>
