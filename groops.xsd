						<xs:element name="startIndex" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>add this normals at index of total matrix (counting from 0)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileArcList" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>to accelerate computation</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="file">
				<xs:annotation>
					<xs:documentation>read normal equations from file</xs:documentation>
					<xs:appinfo>rename: inputfileNormalequation = inputfileNormalEquation</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileNormalEquation" type="filename"/>
						<xs:element name="aprioriSigma" type="double" minOccurs="0" default="1.0"/>
						<xs:element name="startIndex" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>add this normals at index of total matrix (counting from 0)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
