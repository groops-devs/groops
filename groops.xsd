						<xs:element name="R" type="double" minOccurs="0" default="6378136.3">
							<xs:annotation>
								<xs:documentation>reference radius</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="leastSquares" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>false: quadrature formular, true: least squares adjustment order by order</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="GriddedData2SphericalDistance">
				<xs:annotation>
					<xs:documentation>spherical distance between all point pairs of two grids.</xs:documentation>
					<xs:appinfo>tag: Grid</xs:appinfo>
					<xs:appinfo>tag: Matrix</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileMatrix" type="filename">
							<xs:annotation>
								<xs:documentation>matrix containing the spherical distance between all point pairs [rad]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="grid1" type="gridType" maxOccurs="unbounded"/>
						<xs:element name="grid2" type="gridType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="GriddedData2TimeSeries">
				<xs:annotation>
					<xs:documentation>Write time series of gridded data as time series file</xs:documentation>
					<xs:appinfo>tag: Grid</xs:appinfo>
					<xs:appinfo>tag: TimeSeries</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileTimeSeries" type="filename">
							<xs:annotation>
								<xs:documentation>each epoch: multiple data for points (MISCVALUES)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileGriddedData" type="filename" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>file count must agree with number of times</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeSeries" type="timeSeriesType" maxOccurs="unbounded"/>
						<xs:element name="groupDataByPoints" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>multiple data are given point by point, otherwise: data0 for all points, followed by all data1</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="GriddedDataCalculate">
				<xs:annotation>
					<xs:documentation>Calculate values of gridded data.</xs:documentation>
					<xs:appinfo>tag: Grid</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileGriddedData" type="filename" minOccurs="0"/>
						<xs:element name="inputfileGriddedData" type="filename" maxOccurs="unbounded"/>
						<xs:element name="constant" type="expression" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>define a constant by name=value</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="parameter" type="expression" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>define a parameter by name[=value]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="leastSquares" type="expression" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>try to minimize the expression by adjustment of the parameters</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="removalCriteria" type="expression" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>points are removed if one criterion evaluates true. data0 is the first data field.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="longitude" type="expression" default="longitude">
							<xs:annotation>
								<xs:documentation>expression</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="latitude" type="expression" default="latitude">
							<xs:annotation>
								<xs:documentation>expression</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="height" type="expression" default="height">
							<xs:annotation>
								<xs:documentation>expression</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="area" type="expression" minOccurs="0" default="area">
							<xs:annotation>
								<xs:documentation>expression: e.g. deltaL * 2.0 * sin(deltaB/2.0) * cos(latitude/rho)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="value" type="expression" minOccurs="0" default="[&quot;data0&quot;]" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>expression to compute values (input columns are named data0, data1, ...)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="computeArea" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>automatically area computation of rectangular grids (overwrite area)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378137.0">
							<xs:annotation>
								<xs:documentation>reference radius for ellipsoidal coordinates</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inverseFlattening" type="double" minOccurs="0" default="298.2572221010">
							<xs:annotation>
								<xs:documentation>reference flattening for ellipsoidal coordinates</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="statistics" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="outputfile" type="filename">
										<xs:annotation>
											<xs:documentation>matrix with one row, columns are user defined</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="outColumn" type="expression" default="data0rms" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>expression to compute statistics columns, data* are the outputColumns</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="GriddedDataConcatenate">
				<xs:annotation>
					<xs:documentation>Concatenate gridded data from several files</xs:documentation>
					<xs:appinfo>tag: Grid</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileGriddedData" type="filename"/>
						<xs:element name="inputfileGriddedData" type="filename" maxOccurs="unbounded"/>
						<xs:element name="border" type="borderType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="sortPoints" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>sort from north/west to south east</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="removeDuplicates" minOccurs="0">
							<xs:annotation>
								<xs:documentation>remove duplicate points</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element name="keepFirst">
										<xs:annotation>
											<xs:documentation>keep first point, remove all other identicals</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="margin" type="double" minOccurs="0" default="1e-5">
													<xs:annotation>
														<xs:documentation>margin distance for identical points [m]</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="keepLast">
										<xs:annotation>
											<xs:documentation>keep last point, remove all other identicals</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="margin" type="double" minOccurs="0" default="1e-5">
													<xs:annotation>
														<xs:documentation>margin distance for identical points [m]</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378137.0">
							<xs:annotation>
								<xs:documentation>reference radius for ellipsoidal coordinates</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inverseFlattening" type="double" minOccurs="0" default="298.2572221010">
							<xs:annotation>
								<xs:documentation>reference flattening for ellipsoidal coordinates</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="GriddedDataCreate">
				<xs:annotation>
					<xs:documentation>create a grid an write it to file</xs:documentation>
					<xs:appinfo>tag: Grid</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileGrid" type="filename"/>
						<xs:element name="grid" type="gridType" maxOccurs="unbounded"/>
