										</xs:annotation>
									</xs:element>
									<xs:element name="outColumn" type="expression" default="data0rms" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>expression to compute statistics columns, data* are from outColumn</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="InstrumentArcCrossStatistics">
