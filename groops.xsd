	<xs:complexType name="gravityfieldType">
		<xs:annotation>
			<xs:documentation>functions of the gravity field</xs:documentation>
			<xs:appinfo>rename: fromRepresentation = fromParametrization</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="potentialCoefficients">
				<xs:annotation>
					<xs:documentation>file with potential coefficients</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfilePotentialCoefficients" type="filename" default="{groopsDataDir}/potential/"/>
						<xs:element name="minDegree" type="uint" minOccurs="0" default="0"/>
						<xs:element name="maxDegree" type="uint" minOccurs="0"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="setSigmasToZero" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>set variances to zero, should be used by adding back reference fields</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="potentialCoefficientsInterior">
				<xs:annotation>
					<xs:documentation>file with potential coefficients</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfilePotentialCoefficients" type="filename" default="{groopsDataDir}/potential/"/>
						<xs:element name="minDegree" type="uint" minOccurs="0" default="0"/>
						<xs:element name="maxDegree" type="uint" minOccurs="0"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="setSigmasToZero" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>set variances to zero, should be used by adding back reference fields</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="fromParametrization">
				<xs:annotation>
					<xs:documentation>from a solution vector with given parametrization</xs:documentation>
					<xs:appinfo>rename: representation = parametrization</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="parametrization" type="parametrizationGravityType" maxOccurs="unbounded"/>
						<xs:element name="inputfileSolution" type="filename">
							<xs:annotation>
								<xs:documentation>solution vector</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileSigmax" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>standards deviations or covariance matrix of the solution</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="indexStart" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>position in the solution vector</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="rightSide" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>if solution contains several right hand sides, select one</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="timeSplines">
				<xs:annotation>
					<xs:documentation>file with splines in time domain</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileTimeSplinesGravityfield" type="filename" default="{groopsDataDir}/"/>
						<xs:element name="inputfileTimeSplinesCovariance" type="filename" minOccurs="0"/>
						<xs:element name="minDegree" type="uint" minOccurs="0" default="0"/>
						<xs:element name="maxDegree" type="uint" minOccurs="0"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="trend">
				<xs:annotation>
					<xs:documentation>gravityfield as trend</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="gravityfield" type="gravityfieldType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>this field is multiplicated by (time-time0)/timeStep</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeStart" type="time" default="51544.5">
							<xs:annotation>
								<xs:documentation>reference time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeStep" type="time" default="365.25"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="oscillation">
				<xs:annotation>
					<xs:documentation>gravityfield as oscillation</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="gravityfieldCos" type="gravityfieldType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>multiplicated by cos(2pi/T(time-time0))</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gravityfieldSin" type="gravityfieldType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>multiplicated by sin(2pi/T(time-time0))</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="time0" type="time" default="51544.5">
							<xs:annotation>
								<xs:documentation>reference time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="period" type="time" default="365.25">
							<xs:annotation>
								<xs:documentation>[day]</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="inInterval">
				<xs:annotation>
					<xs:documentation>gravityfields only valid in specific time interval</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="gravityfield" type="gravityfieldType" maxOccurs="unbounded"/>
						<xs:element name="timeStart" type="time">
							<xs:annotation>
								<xs:documentation>first point in time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="timeEnd" type="time">
							<xs:annotation>
								<xs:documentation>last point in time will be less or equal timeEnd</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="tides">
				<xs:annotation>
					<xs:documentation>gravityfield from tide models</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="tides" type="tidesType" maxOccurs="unbounded"/>
						<xs:element name="earthRotation" type="earthRotationType"/>
						<xs:element name="ephemerides" type="ephemeridesType" minOccurs="0" default="jpl"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="topography">
				<xs:annotation>
					<xs:documentation>gravityfield from topographic masses</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileGridRectangular" type="filename">
							<xs:annotation>
								<xs:documentation>Digital Terrain Model</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="density" type="expression" minOccurs="0" default="2670">
							<xs:annotation>
								<xs:documentation>expression [kg/m**3]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="radialUpperBound" type="expression" minOccurs="0" default="data0">
							<xs:annotation>
								<xs:documentation>expression (variables &apos;height&apos;, &apos;data&apos;, &apos;L&apos;, &apos;B&apos; and, &apos;area&apos; are taken from the gridded data</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="radialLowerBound" type="expression" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>expression (variables &apos;height&apos;, &apos;data&apos;, &apos;L&apos;, &apos;B&apos; and, &apos;area&apos; are taken from the gridded data</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="distanceMin" type="double" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>[km] min. influence distance (ignore near zone)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="distancePrism" type="double" minOccurs="0" default="15">
							<xs:annotation>
								<xs:documentation>[km] max. distance for prism formular</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="distanceLine" type="double" minOccurs="0" default="100">
							<xs:annotation>
								<xs:documentation>[km] max. distance for radial integration</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="distanceMax" type="double" minOccurs="0">
							<xs:annotation>
								<xs:documentation>[km] max. influence distance (ignore far zone)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="earthquakeOscillation">
				<xs:annotation>
					<xs:documentation>gravityfield from earthquake oscillation</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputCoefficientMatrix" type="filename">
							<xs:annotation>
								<xs:documentation>oscillation model parameters</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="time0" type="time">
							<xs:annotation>
								<xs:documentation>the time earthquake happened</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="minDegree" type="uint" minOccurs="0" default="2"/>
						<xs:element name="maxDegree" type="uint" minOccurs="0" default="2"/>
						<xs:element name="GM" type="double" minOccurs="0" default="3.986004415e+14">
							<xs:annotation>
								<xs:documentation>Geocentric gravitational constant</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378136.3">
							<xs:annotation>
								<xs:documentation>reference radius</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="filter">
				<xs:annotation>
					<xs:documentation>filtered spherical harmonics</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="gravityfield" type="gravityfieldType" maxOccurs="unbounded"/>
						<xs:element name="filter" type="sphericalHarmonicsFilterType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="group">
				<xs:annotation>
					<xs:documentation>group gravityfields</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="gravityfield" type="gravityfieldType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="factor" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>the result is multiplied by this factor, set -1 to subtract the field</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="gridType">
		<xs:annotation>
			<xs:documentation>point distributions on the sphere/ellipsoid</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="geograph">
				<xs:annotation>
					<xs:documentation>along lines of geographical coordinates</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="deltaLambda" type="angle" default="1"/>
						<xs:element name="deltaPhi" type="angle" default="1"/>
