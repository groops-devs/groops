			<xs:element name="GnssResiduals2AccuracyDefinition">
				<xs:annotation>
					<xs:documentation>Compute accuracy definition from observation residuals</xs:documentation>
					<xs:appinfo>rename: outputfileAntennaDefinition = outputfileAntennaMean</xs:appinfo>
					<xs:appinfo>tag: Gnss</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileAccuracyDefinition" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>elevation and azimuth dependent accuracy</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileAntennaMean" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>weighted mean of the residuals</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileAntennaRedundancy" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>redundancy of adjustment</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileAccuracyDefinition" type="filename">
							<xs:annotation>
								<xs:documentation>apriori accuracies</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileStationInfo" type="filename">
							<xs:annotation>
								<xs:documentation>to assign residuals to antennas</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="isTransmitter" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>stationInfo is of a transmitter</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="thresholdOutlier" type="double" minOccurs="0" default="10">
							<xs:annotation>
								<xs:documentation>ignore residuals with sigma/sigma0 greater than threshold</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="minRedundancy" type="double" minOccurs="0" default="3">
							<xs:annotation>
								<xs:documentation>min number of residuals. to estimate sigma</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileResiduals" type="filename" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>GNSS receiver residuals</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
