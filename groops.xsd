					<xs:appinfo>rename: temporalRepresentation = parametrizationTemporal</xs:appinfo>
					<xs:appinfo>rename: intervals = timeIntervals</xs:appinfo>
					<xs:appinfo>tag: Instrument</xs:appinfo>
					<xs:appinfo>tag: Gnss</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileHelmertTimeSeries" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>columns: mjd, Tx,Ty,Tz,s,Rx,Ry,Rz according to temporal parametrization</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="data" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>e.g. satellite, station</xs:documentation>
								<xs:appinfo>rename: startData = startDataFields</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="outputfileInstrument" type="filename" minOccurs="0">
										<xs:annotation>
											<xs:documentation>transformed positions as instrument type Vector3d</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="outputfileInstrumentDiff" type="filename" minOccurs="0">
										<xs:annotation>
											<xs:documentation>position difference as instrument type Vector3d</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="inputfileInstrument" type="filename"/>
									<xs:element name="inputfileInstrumentReference" type="filename"/>
									<xs:element name="startDataFields" type="uint" minOccurs="0" default="0">
										<xs:annotation>
											<xs:documentation>start index of position (x,y,z) columns</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="timeIntervals" type="timeSeriesType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>parameters are estimated per interval</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="parametrizationTemporal" type="parametrizationTemporalType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>temporal parametrization</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="estimateShift" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>coordinate center</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="estimateScale" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>scale factor of position</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="estimateRotation" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>rotation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="huber" type="double" minOccurs="0" default="2.5">
							<xs:annotation>
								<xs:documentation>for robust least squares</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="huberPower" type="double" minOccurs="0" default="1.5">
							<xs:annotation>
								<xs:documentation>for robust least squares</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="huberMaxIteration" type="uint" minOccurs="0" default="30">
							<xs:annotation>
								<xs:documentation>(maximum) number of iterations for robust estimation</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
