					<xs:sequence>
						<xs:element name="inputfileSolfsmy" type="filename" default="{groopsDataDir}/thermosphere/jb2008/SOLFSMY.TXT">
							<xs:annotation>
								<xs:documentation>solar indices</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileDtc" type="filename" default="{groopsDataDir}/thermosphere/jb2008/DTCFILE.TXT"/>
						<xs:element name="inputfileMagnetic3hAp" type="filename" minOccurs="0" default="{groopsDataDir}/thermosphere/hwm14/apActivity.txt">
