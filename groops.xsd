						<xs:element name="parametrization" type="parametrizationGnssAntennaType" maxOccurs="unbounded"/>
						<xs:element name="gnssType" type="gnssType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>e.g. C1CG**</xs:documentation>
							</xs:annotation>
