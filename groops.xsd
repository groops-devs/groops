<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="int">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="uint">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="double">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="angle">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="boolean">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="time">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="doodson">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="string">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="filename">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="expression">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="gnssType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:complexType name="autoregressiveModelSequenceType">
		<xs:sequence>
			<xs:element name="inputfileAutoregressiveModel" type="filename" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>matrix file containing an AR model</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sigma0" type="double" minOccurs="0" default="1.0">
				<xs:annotation>
					<xs:documentation>a-priori sigma for white noise covariance</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="borderType">
		<xs:annotation>
			<xs:documentation>borders of geographical areas</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="rectangle">
				<xs:annotation>
					<xs:documentation>along lines of geographical coordinates</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="minLambda" type="angle"/>
						<xs:element name="maxLambda" type="angle"/>
						<xs:element name="minPhi" type="angle"/>
						<xs:element name="maxPhi" type="angle"/>
						<xs:element name="exclude" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>dismiss points inside</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="cap">
				<xs:annotation>
					<xs:documentation>spherical cap</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="lambdaCenter" type="angle">
							<xs:annotation>
								<xs:documentation>longitude of the center of the cap</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="phiCenter" type="angle">
							<xs:annotation>
								<xs:documentation>latitude of the center of the cap</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="psi" type="angle">
							<xs:annotation>
								<xs:documentation>aperture angle (radius)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="exclude" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>dismiss points inside</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="polygon">
				<xs:annotation>
					<xs:documentation>polygon from file</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfilePolygon" type="filename" default="{groopsDataDir}/border/"/>
						<xs:element name="buffer" type="double" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>buffer around polygon [km], &lt;0: inside</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="exclude" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>dismiss points inside</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="conditionType">
		<xs:choice>
			<xs:element name="fileExist">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="file" type="filename"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="command">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="command" type="filename"/>
						<xs:element name="silently" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>without showing the output.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="expression">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="expression" type="expression"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="matrix">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="matrix" type="matrixGeneratorType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>expression is evaluated for each element of resulting matrix</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="expression" type="expression">
							<xs:annotation>
								<xs:documentation>(variable: data) evaluated for each element</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="all" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>all (=yes)/any (=no) elements must evaluate to true</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="matrixEmpty">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileMatrix" type="filename"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="stringContainsPattern">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="string" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>should contain a {variable}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="pattern" type="filename" minOccurs="0"/>
						<xs:element name="isRegularExpression" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>pattern is  a regular expression</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="caseSensitive" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>treat lower and upper case as distinct</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="stringMatchPattern">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="string" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>should contain a {variable}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="pattern" type="filename" minOccurs="0"/>
						<xs:element name="isRegularExpression" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>pattern is  a regular expression</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="caseSensitive" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>treat lower and upper case as distinct</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="and">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="condition" type="conditionType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="or">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="condition" type="conditionType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="not">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="condition" type="conditionType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="covariancePodType">
		<xs:sequence>
			<xs:element name="sigma" type="double" minOccurs="0" default="1">
				<xs:annotation>
					<xs:documentation>general variance factor</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inputfileSigmasPerArc" type="filename" minOccurs="0">
				<xs:annotation>
					<xs:documentation>different accuaries for each arc (multplicated with sigma)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inputfileSigmasPerEpoch" type="filename" minOccurs="0">
				<xs:annotation>
					<xs:documentation>different accuaries for each epoch (added)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inputfileCovarianceFunction" type="filename" minOccurs="0">
				<xs:annotation>
					<xs:documentation>covariances in time for along, cross, and radial direction</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inputfileCovariancePodEpoch" type="filename" minOccurs="0">
				<xs:annotation>
					<xs:documentation>3x3 epoch wise covariances</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="covarianceSstType">
		<xs:sequence>
			<xs:element name="sigma" type="double" minOccurs="0" default="1">
				<xs:annotation>
					<xs:documentation>general variance factor</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inputfileSigmasPerArc" type="filename" minOccurs="0">
				<xs:annotation>
					<xs:documentation>different accuaries for each arc (multplicated with sigma)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inputfileSigmasPerEpoch" type="filename" minOccurs="0">
				<xs:annotation>
					<xs:documentation>different accuaries for each epoch (added)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inputfileCovarianceFunction" type="filename" minOccurs="0">
				<xs:annotation>
					<xs:documentation>covariance function in time</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inputfileCovarianceMatrixArc" type="filename" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>one matrix file per arc. Use {arcNo} as template</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sigmasCovarianceMatrixArc" type="filename" minOccurs="0">
				<xs:annotation>
					<xs:documentation>vector with one sigma for each covarianceMatrixArc</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="digitalFilterType">
		<xs:annotation>
			<xs:documentation>create digital filter</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="movingAverage">
				<xs:annotation>
					<xs:documentation>moving average (boxcar) filter</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="length" type="uint">
							<xs:annotation>
								<xs:documentation>number of epochs in averaging operator</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inFrequencyDomain" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>apply filter in frequency domain</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="padType">
							<xs:complexType>
								<xs:choice>
									<xs:element name="none">
										<xs:annotation>
											<xs:documentation>no padding is applied</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="zero">
										<xs:annotation>
											<xs:documentation>zero padding</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="constant">
										<xs:annotation>
											<xs:documentation>pad using first and last value</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="periodic">
										<xs:annotation>
											<xs:documentation>periodic continuation of matrix</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="symmetric">
										<xs:annotation>
											<xs:documentation>symmetric continuation around the matrix edges</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="movingMedian">
				<xs:annotation>
					<xs:documentation>moving median filter</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="length" type="uint">
							<xs:annotation>
								<xs:documentation>length of the moving window [epochs]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="padType">
							<xs:complexType>
								<xs:choice>
									<xs:element name="none">
										<xs:annotation>
											<xs:documentation>no padding is applied</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="zero">
										<xs:annotation>
											<xs:documentation>zero padding</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="constant">
										<xs:annotation>
											<xs:documentation>pad using first and last value</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="periodic">
										<xs:annotation>
											<xs:documentation>periodic continuation of matrix</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="symmetric">
										<xs:annotation>
											<xs:documentation>symmetric continuation around the matrix edges</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="derivative">
				<xs:annotation>
					<xs:documentation>differentiation by polynomial approximation</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="polynomialDegree" type="uint" default="8">
							<xs:annotation>
								<xs:documentation>degree of approximation polynomial</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="derivative" type="uint" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>take kth derivative</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="sampling" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>assumed time step between points</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="padType">
							<xs:complexType>
								<xs:choice>
									<xs:element name="none">
										<xs:annotation>
											<xs:documentation>no padding is applied</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="zero">
										<xs:annotation>
											<xs:documentation>zero padding</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="constant">
										<xs:annotation>
											<xs:documentation>pad using first and last value</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="periodic">
										<xs:annotation>
											<xs:documentation>periodic continuation of matrix</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="symmetric">
										<xs:annotation>
											<xs:documentation>symmetric continuation around the matrix edges</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="integral">
				<xs:annotation>
					<xs:documentation>integration by polynomial approximation</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="polynomialDegree" type="uint" default="7">
							<xs:annotation>
								<xs:documentation>degree of approximation polynomial</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="sampling" type="double" minOccurs="0" default="1.0">
							<xs:annotation>
								<xs:documentation>assumed time step between points</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="padType">
							<xs:complexType>
								<xs:choice>
									<xs:element name="none">
										<xs:annotation>
											<xs:documentation>no padding is applied</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="zero">
										<xs:annotation>
											<xs:documentation>zero padding</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="constant">
										<xs:annotation>
											<xs:documentation>pad using first and last value</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="periodic">
										<xs:annotation>
											<xs:documentation>periodic continuation of matrix</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="symmetric">
										<xs:annotation>
											<xs:documentation>symmetric continuation around the matrix edges</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="correlation">
				<xs:annotation>
					<xs:documentation>correlation by simple coefficient</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="correlation" type="double">
							<xs:annotation>
								<xs:documentation>correlation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="backwardDirection" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>apply filter in backward direction</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inFrequencyDomain" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>apply filter in frequency domain</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="padType">
							<xs:complexType>
								<xs:choice>
									<xs:element name="none">
										<xs:annotation>
											<xs:documentation>no padding is applied</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="zero">
										<xs:annotation>
											<xs:documentation>zero padding</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="constant">
										<xs:annotation>
											<xs:documentation>pad using first and last value</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="periodic">
										<xs:annotation>
											<xs:documentation>periodic continuation of matrix</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="symmetric">
										<xs:annotation>
											<xs:documentation>symmetric continuation around the matrix edges</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="graceLowpass">
				<xs:annotation>
					<xs:documentation>GRACE low pass filter (self convolving kernel)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rawDataRate" type="double" default="10.0">
							<xs:annotation>
								<xs:documentation>sampling frequency in Hz (fs).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="convolutionNumber" type="uint" minOccurs="0" default="7">
							<xs:annotation>
								<xs:documentation>number of self convolutions of the filter kernel</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="fitInterval" type="double" minOccurs="0" default="70.7">
							<xs:annotation>
								<xs:documentation>length of the filter kernel [seconds]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="lowPassBandwith" type="double" minOccurs="0" default="0.1">
							<xs:annotation>
								<xs:documentation>target low pass bandwidth</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="normFrequency" type="double" minOccurs="0" default="0.37e-3">
							<xs:annotation>
								<xs:documentation>norm filter at this frequency [Hz] (default: GRACE dominant (J2) signal frequency)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="reduceQuadraticFit" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>remove-&gt;filter-&gt;restore quadratic fit</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="derivative" minOccurs="0">
							<xs:complexType>
								<xs:choice>
									<xs:element name="derivative1st">
										<xs:annotation>
											<xs:documentation>range rate</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="derivative2nd">
										<xs:annotation>
											<xs:documentation>range acceleration</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="inFrequencyDomain" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>apply filter in frequency domain</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="padType">
							<xs:complexType>
								<xs:choice>
									<xs:element name="none">
										<xs:annotation>
											<xs:documentation>no padding is applied</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="zero">
										<xs:annotation>
