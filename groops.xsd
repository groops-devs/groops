						<xs:element name="timeIntervals" type="timeSeriesType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>for {loopTime} variable in inputfile</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="factor" type="double" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>e.g. sqrt(3) for 3D RMS</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="estimateShift" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>coordinate center every epoch</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="estimateScale" type="boolean" minOccurs="0" default="1">
