						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Matrix2GriddedData">
				<xs:annotation>
					<xs:documentation>Read gridded data from matrix</xs:documentation>
					<xs:appinfo>tag: Grid</xs:appinfo>
					<xs:appinfo>tag: Matrix</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileGriddedData" type="filename"/>
						<xs:element name="inputfileMatrix" type="filename"/>
						<xs:element name="points">
							<xs:complexType>
								<xs:choice>
									<xs:element name="ellipsoidal">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="longitude" type="expression" default="data0">
													<xs:annotation>
														<xs:documentation>expression</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="latitude" type="expression" default="data1">
													<xs:annotation>
														<xs:documentation>expression</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="height" type="expression" default="data2">
													<xs:annotation>
														<xs:documentation>expression</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="cartesian">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="x" type="expression" default="data0">
													<xs:annotation>
														<xs:documentation>expression</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="y" type="expression" default="data1">
													<xs:annotation>
														<xs:documentation>expression</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="z" type="expression" default="data2">
													<xs:annotation>
														<xs:documentation>expression</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="area" type="expression" minOccurs="0">
							<xs:annotation>
								<xs:documentation>expression (e.g. deltaL*2*sin(deltaB/2)*cos(data1/RHO))</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="value" type="expression" minOccurs="0" default="[&quot;data3&quot;]" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>expression</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="sortPoints" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>sort from north/west to south east</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="computeArea" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>the area can be computed automatically for rectangular grids</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378137.0">
							<xs:annotation>
								<xs:documentation>reference radius for ellipsoidal coordinates</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inverseFlattening" type="double" minOccurs="0" default="298.2572221010">
							<xs:annotation>
								<xs:documentation>reference flattening for ellipsoidal coordinates</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="MatrixRectangular2GriddedData">
				<xs:annotation>
					<xs:documentation>read gridded data (matrix)</xs:documentation>
					<xs:appinfo>tag: Grid</xs:appinfo>
					<xs:appinfo>tag: Matrix</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileGriddedData" type="filename"/>
						<xs:element name="inputfileMatrix" type="filename"/>
						<xs:element name="rowMajor" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>true: data is ordered row by row, false: columnwise</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="startLongitude" type="angle">
							<xs:annotation>
								<xs:documentation>longitude of upper left corner of the grid</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="startLatitude" type="angle">
							<xs:annotation>
								<xs:documentation>latitude of upper left corner of the grid</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="deltaLongitude" type="angle">
							<xs:annotation>
								<xs:documentation>sampling, negative for east to west data</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="deltaLatitude" type="angle">
							<xs:annotation>
								<xs:documentation>sampling, negative for south to north data</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378137.0">
							<xs:annotation>
								<xs:documentation>reference radius for ellipsoidal coordinates</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inverseFlattening" type="double" minOccurs="0" default="298.2572221010">
							<xs:annotation>
								<xs:documentation>reference flattening for ellipsoidal coordinates</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="TimeSeries2GriddedData">
				<xs:annotation>
					<xs:documentation>Write time series as gridded data for each epoch</xs:documentation>
					<xs:appinfo>tag: Grid</xs:appinfo>
					<xs:appinfo>tag: TimeSeries</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfilesGriddedData" type="filename" default="grid_{loopTime:%y-%m}.dat">
							<xs:annotation>
								<xs:documentation>for each epoch</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopTime" type="string" minOccurs="0" default="loopTime">
							<xs:annotation>
								<xs:documentation>variable with time of each epoch</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopIndex" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>variable with index of current epoch (starts with zero)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopCount" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>variable with total number of epochs</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileTimeSeries" type="filename">
							<xs:annotation>
								<xs:documentation>each epoch: multiple data for points (MISCVALUES)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="grid" type="gridType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>corresponding grid points</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="isDataGroupedByPoint" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>multiple data are given point by point, otherwise: first data0 for all points, followed by all data1</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="R" type="double" minOccurs="0" default="6378137.0">
							<xs:annotation>
								<xs:documentation>reference radius for ellipsoidal coordinates on output</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inverseFlattening" type="double" minOccurs="0" default="298.2572221010">
							<xs:annotation>
								<xs:documentation>reference flattening for ellipsoidal coordinates on output, 0: spherical coordinates</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Instrument2AllanVariance">
				<xs:annotation>
					<xs:documentation>Compute Allan variance from instrument files.</xs:documentation>
					<xs:appinfo>tag: Instrument</xs:appinfo>
					<xs:appinfo>tag: Covariance</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileAllanVariance" type="filename">
							<xs:annotation>
								<xs:documentation>column 0: averaging interval [seconds], column 1-(n-1): Allan variance for each data column</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileInstrument" type="filename"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Instrument2CovarianceFunctionVCE">
				<xs:annotation>
					<xs:documentation>Covariance functions via variance component estimation</xs:documentation>
					<xs:appinfo>tag: Instrument</xs:appinfo>
					<xs:appinfo>tag: Covariance</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileCovarianceFunction" type="filename">
							<xs:annotation>
								<xs:documentation>covariance functions</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileSigmasPerArc" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>accuracies of each arc</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileResiduals" type="filename" minOccurs="0"/>
						<xs:element name="outputfileSolution" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>estimated parameter vector (global part only)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileInstrument" type="filename"/>
						<xs:element name="startDataFields" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>start</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="countDataFields" type="uint" minOccurs="0">
							<xs:annotation>
								<xs:documentation>number of data fields (default: all after start)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="parameter" type="parametrizationTemporalType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>data is reduced by temporal representation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="parameterPerArc" type="parametrizationTemporalType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>data is reduced by temporal representation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="iterationCount" type="uint" minOccurs="0" default="5">
							<xs:annotation>
								<xs:documentation>number of iterations for the estimation</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Instrument2CrossCorrelationFunction">
				<xs:annotation>
					<xs:documentation>Empirical computation of the correlation between two instrument files.</xs:documentation>
					<xs:appinfo>tag: Instrument</xs:appinfo>
					<xs:appinfo>tag: Statistics</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileCorrelation" type="filename">
							<xs:annotation>
								<xs:documentation>column 1: time lag, column 2..n cross-correlation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileInstrument" type="filename"/>
						<xs:element name="inputfileInstrumentReference" type="filename"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Instrument2Histogram">
				<xs:annotation>
					<xs:documentation>compute a histogram from an instrument file</xs:documentation>
					<xs:appinfo>tag: Instrument</xs:appinfo>
					<xs:appinfo>tag: Statistics</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileMatrix" type="filename">
							<xs:annotation>
								<xs:documentation>column 1: lower bin bound; columns 2 to N: histogram of each arc</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileInstrument" type="filename"/>
						<xs:element name="selectDataField" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>select channel for histogram computation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="binCount" type="uint" minOccurs="0">
							<xs:annotation>
								<xs:documentation>(default: Freedman-Diaconis&apos; choice, maximum of all channels)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="lowerBound" type="expression" minOccurs="0" default="dataMin">
							<xs:annotation>
								<xs:documentation>lower bound for bins (default: global minimum, data values outside are ignored)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="upperBound" type="expression" minOccurs="0" default="dataMax">
							<xs:annotation>
								<xs:documentation>upper bound for bins (default: global maximum, data values outside are ignored)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="relative" type="boolean" minOccurs="0" default="1">
							<xs:annotation>
								<xs:documentation>output relative frequencies</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="cumulative" type="boolean" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>accumulate frequencies</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Instrument2PowerSpectralDensity">
				<xs:annotation>
					<xs:documentation>Compute PSD from instrument files.</xs:documentation>
					<xs:appinfo>tag: Instrument</xs:appinfo>
					<xs:appinfo>tag: Covariance</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfilePSD" type="filename">
							<xs:annotation>
								<xs:documentation>estimated PSD: column 0: frequency vector, column 1-(n-1): PSD estimate for each channel</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileInstrument" type="filename"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Instrument2RmsPlotGrid">
				<xs:annotation>
					<xs:documentation>Compute RMS plot grid from instrument file(s) containing 3D data (e.g. orbits, station positions).</xs:documentation>
					<xs:appinfo>rename: intervals = timeIntervals</xs:appinfo>
					<xs:appinfo>tag: Instrument</xs:appinfo>
					<xs:appinfo>tag: Plot</xs:appinfo>
					<xs:appinfo>tag: Gnss</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="outputfileRmsPlotGrid" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>columns: mjd, id, rms</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="outputfileHelmertTimeSeries" type="filename" minOccurs="0">
							<xs:annotation>
								<xs:documentation>columns: mjd, tx, ty, tz, scale, rx, ry, rz</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileInstrument" type="filename" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>one file per satellite/station</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileInstrumentReference" type="filename" maxOccurs="unbounded">
