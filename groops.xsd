	<xs:complexType name="interpolatorTimeSeriesType">
		<xs:annotation>
			<xs:documentation>resampling method</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="polynomial">
				<xs:annotation>
					<xs:documentation>polynomial prediction</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="polynomialDegree" type="uint" default="3">
							<xs:annotation>
								<xs:documentation>degree of the moving polynomial</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxDataPointRange" type="double" default="-(3+1.1)">
							<xs:annotation>
								<xs:documentation>[seconds] all degree+1 data points must be within this range for a valid polynomial</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxExtrapolationDistance" type="double" minOccurs="0" default="-1.1">
							<xs:annotation>
								<xs:documentation>[seconds] resampling points within this range of the polynomial will be extrapolated</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="leastSquaresPolynomialFit">
				<xs:annotation>
					<xs:documentation>least squares polynomial fit</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="polynomialDegree" type="uint">
							<xs:annotation>
								<xs:documentation>degree of the estimated polynomial</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxDataPointDistance" type="double">
							<xs:annotation>
								<xs:documentation>[seconds] all data points within this distance around the resampling point will be used</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxExtrapolationDistance" type="double" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>[seconds] resampling points within this range of the polynomial will be extrapolated</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="fillGapsLeastSquaresPolynomialFit">
				<xs:annotation>
					<xs:documentation>least squares polynomial fit</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="polynomialDegree" type="uint" minOccurs="0" default="3">
							<xs:annotation>
								<xs:documentation>degree of the estimated polynomial</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxDataGap" type="double" minOccurs="0" default="100">
							<xs:annotation>
								<xs:documentation>[seconds] max data gap to interpolate</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxDataSpan" type="double" minOccurs="0" default="20">
							<xs:annotation>
								<xs:documentation>[seconds] time span on each side used for least squares fit</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="margin" type="double" minOccurs="0" default="1e-5">
							<xs:annotation>
								<xs:documentation>[seconds] margin for identical times</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="kernelType">
		<xs:annotation>
			<xs:documentation>harmonic and isotropic integral kernels</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="geoidHeight">
				<xs:annotation>
					<xs:documentation>geoid = potential/normalgravity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="anomalies">
				<xs:annotation>
					<xs:documentation>gravity anomalies, Stokes kernel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="disturbance">
				<xs:annotation>
					<xs:documentation>gravity disturbance, Hotine kernel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="potential">
				<xs:annotation>
					<xs:documentation>Abel-Poisson kernel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="density">
				<xs:annotation>
					<xs:documentation>mass on a single layer (1/l kernel)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileLoadingLoveNumber" type="filename" minOccurs="0" default="{groopsDataDir}/loading/loadLoveNumbers_Gegout97.txt"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="waterHeight">
				<xs:annotation>
					<xs:documentation>equivalent water columns, accounts the loading</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="density" type="double" default="1025">
							<xs:annotation>
								<xs:documentation>[kg/m**3]</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inputfileLoadingLoveNumber" type="filename" minOccurs="0" default="{groopsDataDir}/loading/loadLoveNumbers_Gegout97.txt"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="bottomPressure">
				<xs:annotation>
					<xs:documentation>ocean bottom pressure in Pascal, accounts for loading</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileLoadingLoveNumber" type="filename" minOccurs="0" default="{groopsDataDir}/loading/loadLoveNumbers_Gegout97.txt"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="deformation">
				<xs:annotation>
					<xs:documentation>radial deformation by loading</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileDeformationLoadLoveNumber" type="filename" default="{groopsDataDir}/loading/deformationLoveNumbers_CM_Gegout97.txt"/>
						<xs:element name="inputfilePotentialLoadLoveNumber" type="filename" minOccurs="0" default="{groopsDataDir}/loading/loadLoveNumbers_Gegout97.txt">
							<xs:annotation>
								<xs:documentation>if full potential is given and not only loading potential</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="radialGradient">
				<xs:annotation>
					<xs:documentation>radial gravity gradient</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="coefficients">
				<xs:annotation>
					<xs:documentation>construct kernel by a legendre-polynomial expansion</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="inputfileCoefficients" type="filename"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="filterGauss">
				<xs:annotation>
					<xs:documentation>smoothing by a gauss filter</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="kernel" type="kernelType"/>
						<xs:element name="radius" type="double" default="500">
							<xs:annotation>
								<xs:documentation>filter radius [km]</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="blackmanLowPass">
				<xs:annotation>
					<xs:documentation>Blackman low-pass filter</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="kernel" type="kernelType"/>
						<xs:element name="startDegreeTransition" type="uint">
							<xs:annotation>
								<xs:documentation>minimum degree in transition band</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="stopDegreeTransition" type="uint">
							<xs:annotation>
								<xs:documentation>maximum degree in transition band</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="truncation">
				<xs:annotation>
					<xs:documentation>truncate kernel at specific degree</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="kernel" type="kernelType"/>
						<xs:element name="minDegree" type="uint" minOccurs="0" default="0">
							<xs:annotation>
								<xs:documentation>truncate before minDegree</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxDegree" type="uint">
							<xs:annotation>
								<xs:documentation>truncate after maxDegree</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="selenoidHeight">
				<xs:annotation>
					<xs:documentation>selenoid = potential/normalgravity</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="loopType">
		<xs:annotation>
			<xs:appinfo>rename: temporal = timeIntervals</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="timeSeries">
				<xs:annotation>
					<xs:documentation>Loop over points in time</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="timeSeries" type="timeSeriesType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>loop is called for every point in time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopTime" type="string" minOccurs="0" default="loopTime">
							<xs:annotation>
								<xs:documentation>variable with time of each loop</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopIndex" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>variable with index of current iteration (starts with zero)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableLoopCount" type="string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>variable with total number of iterations</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="condition" type="conditionType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>check before each loop step</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="timeIntervals">
				<xs:annotation>
					<xs:documentation>Loop over time intervals</xs:documentation>
				</xs:annotation>
